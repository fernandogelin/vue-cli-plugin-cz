{
  "name": "vue-cli-plugin-cz",
  "version": "0.0.1",
  "description": "vue-cli plugin to set up commitizen and semantic release.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandogelin/vue-cli-plugin-cz.git"
  },
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "semantic-release --generate-notes"
  },
  "keywords": [
    "commitizen",
    "conventional commits",
    "semantic release",
    "vue",
    "github actions"
  ],
  "author": "Fernando Gelin",
  "license": "MIT",
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "git-cz": "^4.3.1",
    "semantic-release": "^17.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
